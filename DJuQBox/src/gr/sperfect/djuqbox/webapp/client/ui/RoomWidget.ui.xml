<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:gr.sperfect.djuqbox.webapp.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		roomL player, controls, playlist, users, history, actions ......
		<g:Button styleName="{style.important}" ui:field="button" />
		<my:PlayerWidget ui:field='player' />
		<my:PlayerControls ui:field='playerControls' />
		<g:DecoratedStackPanel width="100%" height="100%">
			<my:SearchSongWidget ui:field='searchSong' width="100%"
				height="100%" g:StackPanel-text="search" />
			<my:PlaylistWidget ui:field="playList" width="100%"
				height="100%" g:StackPanel-text="playlist" />
		</g:DecoratedStackPanel>


	</g:HTMLPanel>
</ui:UiBinder> 